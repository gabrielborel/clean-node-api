{
  "name": "ts-tdd-api",
  "version": "1.2.0",
  "description": "An API to practice TDD, Clean Architecture and SOLID principles using TypeScript",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rm -rf dist && tsc",
    "debug": "node --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "dev": "npm run build && docker-compose up",
    "test": "npx vitest --run --silent --passWithNoTests ",
    "test:verbose": "npx vitest",
    "test:integration": "npx vitest -c vitest-integration.config.ts",
    "test:unit": "npx vitest -c vitest-unit.config.ts"
  },
  "keywords": [],
  "author": "Gabriel Borel",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.2.5",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "git-commit-msg-linter": "^5.0.4",
    "mongodb-memory-server": "^8.13.0",
    "supertest": "^6.3.3",
    "typescript": "^5.1.3",
    "vitest-tsconfig-paths": "^3.4.1",
    "vitest": "^0.32.0",
    "tsx": "^3.12.7"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.6.0",
    "validator": "^13.9.0"
  },
  "engines": {
    "node": "18.x"
  }
}
