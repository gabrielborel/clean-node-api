{
  "name": "ts-tdd-api",
  "version": "2.0.1",
  "description": "An API to practice TDD, Clean Architecture and SOLID principles using TypeScript",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rm -rf dist && tsc -p tsconfig-build.json",
    "debug": "npx nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "dev": "npm run build && docker-compose up",
    "test": "jest --passWithNoTests --silent --noStackTrace",
    "test:verbose": "jest --passWithNoTests",
    "test:integration": "npm test -- --watch -c jest-integration.config.ts",
    "test:unit": "npm test -- --watch -c jest-unit.config.ts"
  },
  "keywords": [],
  "author": "Gabriel Borel",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.7",
    "@swc/core": "^1.3.68",
    "@swc/jest": "^0.2.26",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.2.5",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "@vitest/coverage-v8": "^0.32.2",
    "git-commit-msg-linter": "^5.0.4",
    "jest": "^29.6.1",
    "mongodb-memory-server": "^8.13.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.3",
    "mongodb": "^5.6.0",
    "nodemon": "^2.0.22",
    "validator": "^13.9.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
